<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jochem.ghostapp" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:screenOrientation="portrait">
        <activity
            android:name="nl.mprog.Ghost.ghostapp.StartScreen"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="nl.mprog.Ghost.ghostapp.EndScreen"
            android:label="@string/title_activity_end_screen" >
        </activity>
        <activity
            android:name="nl.mprog.Ghost.ghostapp.MainActivity"
            android:label="@string/title_activity_start_screen" >

        </activity>
        <activity
            android:name="nl.mprog.Ghost.ghostapp.HighScores"
            android:label="@string/title_activity_high_scores" >
        </activity>
    </application>

</manifest>
